<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<add name="ConnectionString" connectionString="Data Source=Roger;Initial Catalog=app;Integrated Security=True"/>
	</connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="Database" value="app"/>
		<add key="ParentTableName" value="areas"/>
		<add key="ChildTableName" value="projects"/>
		<add key="SelectChild" value="select * from projects"/>
		<add key="SelectParent" value="select * from areas"/>
		<add key="ChildForeignKey" value="area_id"/>
		<add key="ParentReferencedKey" value="area_id"/>
		<add key="ForeignKey" value="fk_area"/>
	</appSettings>
</configuration>